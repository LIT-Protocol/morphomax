{
  "name": "morphomax-backend",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "^20.11.1",
    "pnpm": "10.7.0"
  },
  "packageManager": "pnpm@10.7.0",
  "scripts": {
    "graphql:schema": "pnpm dlx get-graphql-schema https://blue-api.morpho.org/graphql > src/lib/graphql/schema.graphql",
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "graphql": "pnpm run graphql:schema && pnpm run graphql:codegen",
    "test": "pnpm jest",
    "lint": "pnpm eslint ./src",
    "build": "pnpm unbuild",
    "esbuild": "node esbuild.mjs",
    "dev": "pnpm run graphql && pnpm dotenvx run -- pnpm bun --watch src/bin/serverWorker.ts",
    "start": "node ./dist/bin/serverWorker.mjs",
    "startWorker": "node ./dist/bin/jobWorker.mjs",
    "startApiServer": "node ./dist/bin/apiServer.mjs",
    "clean": "pnpm exec rm -Rf node_modules dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LIT-Protocol/morphomax.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "serverWorker": "./dist/bin/serverWorker.mjs",
    "jobWorker": "./dist/bin/jobWorker.mjs",
    "apiServer": "./dist/bin/apiServer.mjs"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@dotenvx/dotenvx": "^1.39.0",
    "@lit-protocol/auth-helpers": "7.2.0",
    "@lit-protocol/constants": "7.2.0",
    "@lit-protocol/contracts-sdk": "7.2.0",
    "@lit-protocol/lit-node-client": "7.2.0",
    "@lit-protocol/types": "7.2.0",
    "@lit-protocol/vincent-ability-erc20-approval": "0.0.12-mma",
    "@lit-protocol/vincent-ability-erc20-transfer": "0.0.12-mma",
    "@lit-protocol/vincent-ability-morpho": "0.0.12-mma",
    "@lit-protocol/vincent-ability-uniswap-swap": "0.0.12-mma",
    "@lit-protocol/vincent-app-sdk": "0.0.12-mma",
    "@lit-protocol/vincent-scaffold-sdk": "1.1.9-mma",
    "@morpho-org/blue-api-sdk": "^2.8.1",
    "@morpho-org/blue-sdk": "^4.2.0",
    "@morpho-org/morpho-ts": "^2.4.1",
    "@morpho-org/simulation-sdk": "^3.1.1",
    "@noble/secp256k1": "^2.2.3",
    "@t3-oss/env-core": "^0.12.0",
    "@whisthub/agenda": "^7.0.2",
    "consola": "^3.4.2",
    "cors": "^2.8.5",
    "dataloader": "^2.2.3",
    "did-jwt": "^8.0.9",
    "ethers": "^5.8.0",
    "express": "^4.21.2",
    "mongoose": "^8.10.1",
    "node-cache": "^5.1.2",
    "viem": "^2.33.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typed-document-node": "^5.1.2",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "bun": "^1.2.6",
    "esbuild": "^0.25.1",
    "graphql": "^16.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.0",
    "unbuild": "^3.5.0"
  }
}
